<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
>

          <Table  id="_IDGenTable2" headerText="ModeloPartidas/nome"
                      items="{ModeloPartidas>/partidas}"
                        inset="false">
            <columns>
              <Column id="_IDGenColumn12" width="10%" hAlign="Right">
                <Text id="_IDGenText202" text="" />
              </Column>
              <Column id="_IDGenColumn13" width="10%" hAlign="Center">
                <Text id="_IDGenText203" text="Resultados" />
              </Column>
              <Column id="_IDGenColumn14" width="10%" hAlign="Left">
                <Text id="_IDGenText205" text="" />
              </Column>
            </columns>
            <ColumnListItem  id="_IDGenColumnListItem21"  >
              <cells >
                <Image  id="_IDGenImage20" width="10%" 
                                 src="{ModeloPartidas>time_mandante/escudo}"></Image>
                <VBox id="_IDGenVBox1" >
                  <Text  id="_IDGenText204"  text="{ModeloPartidas>placar}"></Text>
                  <Text  id="_IDGenText206"  text="{ModeloPartidas>data_realizacao} {ModeloPartidas>hora_realizacao}"></Text>
                  <Text  id="_IDGenText207"  text="{ModeloPartidas>status}"></Text>
                </VBox>
                <Image  id="_IDGenImage21" width="10%" 
                                     src="{ModeloPartidas>time_visitante/escudo}"></Image>
              </cells>
            </ColumnListItem>
          </Table>
</c:FragmentDefinition>